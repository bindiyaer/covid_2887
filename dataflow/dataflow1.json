{
	"name": "dataflow1",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "ds_target_circuits",
						"type": "DatasetReference"
					},
					"name": "circuits"
				},
				{
					"dataset": {
						"referenceName": "ds_target_constructors",
						"type": "DatasetReference"
					},
					"name": "source1"
				}
			],
			"sinks": [],
			"transformations": [
				{
					"name": "select1"
				}
			],
			"scriptLines": [
				"source(output(",
				"          circuitId as string,",
				"          circuitRef as string,",
				"          name as string,",
				"          location as string,",
				"          country as string,",
				"          lat as string,",
				"          lng as string,",
				"          alt as string,",
				"          url as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> circuits",
				"source(output(",
				"          constructorId as string,",
				"          constructorRef as string,",
				"          name as string,",
				"          nationality as string,",
				"          url as string",
				"     ),",
				"     allowSchemaDrift: false,",
				"     validateSchema: true,",
				"     limit: 100,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     query: '',",
				"     format: 'query') ~> source1",
				"circuits select(mapColumn(",
				"          circuit_id = circuitId,",
				"          circuit_ref = circuitRef,",
				"          name,",
				"          location,",
				"          country,",
				"          latitude = lat,",
				"          longitude = lng,",
				"          altitude = alt",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> select1"
			]
		}
	}
}