{
	"name": "dataflow1",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "ds_target_circuits",
						"type": "DatasetReference"
					},
					"name": "circuits"
				},
				{
					"dataset": {
						"referenceName": "ds_target_results",
						"type": "DatasetReference"
					},
					"name": "results"
				}
			],
			"sinks": [],
			"transformations": [
				{
					"name": "sel"
				}
			],
			"scriptLines": [
				"source(output(",
				"          circuitId as string,",
				"          circuitRef as string,",
				"          name as string,",
				"          location as string,",
				"          country as string,",
				"          lat as string,",
				"          lng as string,",
				"          alt as string,",
				"          url as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> circuits",
				"source(output(",
				"          resultId as string,",
				"          raceId as string,",
				"          driverId as string,",
				"          constructorId as string,",
				"          number as string,",
				"          grid as string,",
				"          position as string,",
				"          positionText as string,",
				"          positionOrder as string,",
				"          points as string,",
				"          laps as string,",
				"          time as string,",
				"          milliseconds as string,",
				"          fastestLap as string,",
				"          rank as string,",
				"          fastestLapTime as string,",
				"          fastestLapSpeed as string,",
				"          statusId as string",
				"     ),",
				"     allowSchemaDrift: false,",
				"     validateSchema: true,",
				"     limit: 100,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> results",
				"results select(mapColumn(",
				"          constructor_id = constructorId,",
				"          constructor_ref = constructorRef,",
				"          name,",
				"          nationality",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> sel"
			]
		}
	}
}